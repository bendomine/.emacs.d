 '(pixel-scroll-mode t)
 '(pixel-scroll-precision-mode t)
 '(ring-bell-function 'ignore)
 '(scroll-bar-mode nil)
 '(send-mail-function 'mailclient-send-it)
 '(tab-bar-new-tab-choice "*GNU Emacs*")
 '(yas-global-mode t))
